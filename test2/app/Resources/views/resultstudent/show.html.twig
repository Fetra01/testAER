{% extends 'base.html.twig' %}

{% block body %}
    <h1>Resultstudent</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ resultStudent.id }}</td>
            </tr>
            <tr>
                <th>Student</th>
                <td>{{ resultStudent.student }}</td>
            </tr>
            <tr>
                <th>Survey</th>
                <td>{{ resultStudent.survey }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('resultstudent_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('resultstudent_edit', { 'id': resultStudent.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
